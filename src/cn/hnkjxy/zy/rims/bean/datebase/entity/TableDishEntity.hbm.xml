<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.hnkjxy.zy.rims.bean.datebase.entity.TableDishEntity" table="t_dish" schema="zy_rims">
        <id name="dishId" column="dish_id">
            <generator class="identity"/>
        </id>
        <property name="dishName" column="dish_name"/>
        <property name="dishPrice" column="dish_price"/>
        <property name="dishImgName" column="dish_img_name"/>
        <property name="dishIntroduced" column="dish_introduced"/>
    </class>
</hibernate-mapping>