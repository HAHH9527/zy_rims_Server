<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.hnkjxy.zy.rims.bean.TOrderEntity" table="t_order" schema="zy_rims">
        <id name="orderId" column="order_id"/>
        <property name="tableId" column="table_id"/>
        <property name="orderStatus" column="order_status"/>
        <property name="useTime" column="use_time"/>
        <property name="endTime" column="end_time"/>
        <property name="dishList" column="dish_list"/>
        <property name="orderPrice" column="order_price"/>
        <property name="orderDiscount" column="order_discount"/>
        <property name="orderPriceReal" column="order_price_real"/>
        <property name="orderOther" column="order_other"/>
    </class>
</hibernate-mapping>